<Window x:Class="Mpbis.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mpbis"
        mc:Ignorable="d"
        Title="UserWindow" Height="450" Width="800">
   
    <Canvas RenderTransformOrigin="0.531,0.575" Background="Black">

        <Button Content="话费查询" Name="btn1"  Canvas.Left="109" Canvas.Top="161" Width="192" Height="101" RenderTransformOrigin="2.198,-0.684" Background="#FFAD4E4E" FontSize="36"/>
        <Button Content="业务办理" Name="btn2" Canvas.Left="431" Canvas.Top="161" Width="206" Height="101" FontSize="36" Background="#FFB62C2C"/>


        <Ellipse Fill="Blue"
                         Width="100" Height="100"
                         Stroke="Bisque" StrokeThickness="2" Canvas.Left="684" Canvas.Top="10"/>
        <Expander Canvas.Left="694" Canvas.Top="82">
            <StackPanel>
                <Button Name="again">重新登录</Button>
                <Button Name="exitbtn">退出</Button>
                
            </StackPanel>
        </Expander>



        <Frame Name="frame"></Frame>
    </Canvas>

</Window>
