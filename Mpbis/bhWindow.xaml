<Window x:Class="Mpbis.bhWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mpbis"
        mc:Ignorable="d"
        Title="bhWindow" Height="450" Width="800">
    <StackPanel>
        <Label HorizontalContentAlignment="Center" FontSize="25">业务办理</Label>

        <ListBox Name="listBox">

        </ListBox>
       
        <DockPanel Visibility="Collapsed" Name="dockPanel">
            <StackPanel>
                <Label Name="label"></Label>
                <StackPanel Height="2" Background="Black"></StackPanel>
                <WrapPanel HorizontalAlignment="Center">
                    <Button DockPanel.Dock="Bottom" Name="btn1" Margin="0,10,10,0">确定购买</Button>
                    <Button Name="backbtn" Margin="10,10,0,0">返回</Button>
                </WrapPanel>
            </StackPanel>
        </DockPanel>
        

        <Button HorizontalAlignment="Center" Name="backbtn1">返回</Button>




    </StackPanel>
</Window>
